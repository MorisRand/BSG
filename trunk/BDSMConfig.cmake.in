# - Config file for the FooBar package
# It defines the following variables
#  BDSM_INCLUDE_DIRS - include directories for FooBar
#  BDSM_LIBRARIES    - libraries to link against
#  BDSM_EXECUTABLE   - the bar executable
 
# Compute paths
get_filename_component(BDSM_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(BDSM_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET NME AND NOT BDSM_BINARY_DIR)
  include("${BDSM_CMAKE_DIR}/BDSMTargets.cmake")
endif()
 
# These are IMPORTED targets created by BDSMTargets.cmake
set(BDSM_LIBRARIES NME)
set(BDSM_EXECUTABLE BDSM)
